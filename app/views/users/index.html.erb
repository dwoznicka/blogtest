<h1> Users' list</h1>
<br />
<%= link_to "Create new user", new_user_path %> 
<br />
<ul> 
    <% @users.each do |user| %> 
        <li> 
            <%= user.login + " " + user.email + " " + user.age.to_s + " " %> 
            <%= link_to "Show", user%> <%= link_to "Edit", edit_user_path(user)%> 
            <%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %>
        </li> 
    <% end %> 
</ul>