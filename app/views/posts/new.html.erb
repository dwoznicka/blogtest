<h1>Create new post</h1>
<%= form_for(@post) do |f| %> 

  <div class="field">
      <%= f.label :title %> 
      <%= f.text_field :title %>
  </div>

  <div class="field">
      <%= f.label :content %>
      <%= f.text_area :content %>
  </div>
  
  <div class="field">
    <%= f.label :user_id %><br>
    <%= f.number_field :user_id %>
  </div>

  <div class="actions">
      <%= f.submit "Add!" %> 
  </div>
<% end %>
